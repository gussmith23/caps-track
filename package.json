{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css-once": "node_modules/.bin/sass scss/style.scss src/app/style.css",
    "dev": "npm run css-once && node --require reflect-metadata ./node_modules/.bin/next dev",
    "build": "npm run css-once && next build",
    "start": "node --require reflect-metadata ./node_modules/.bin/next start",
    "lint": "next lint"
  },
  "dependencies": {
    "better-sse": "^0.15.1",
    "bootstrap": "^5.3.6",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.89.1",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/es6-shim": "^0.31.45",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "browser": {
    "typeorm": false
  }
}
